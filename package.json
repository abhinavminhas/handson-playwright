{
  "name": "handson-playwright",
  "version": "1.0.0",
  "description": "playwright handson project",
  "main": "index.js",
  "scripts": {
    "install": "npx playwright install --with-deps",
    "install:chromium": "npx playwright install --with-deps chromium",
    "install:firefox": "npx playwright install --with-deps firefox",
    "install:webkit": "npx playwright install --with-deps webkit",
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "test:debug": "npx playwright test --debug",
    "test:report": "npx playwright show-report"
  },
  "keywords": [
    "playwright", "testing"
   ],
  "author": "Abhinav Minhas",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.9.1",
    "dotenv": "^17.2.3"
  }
}
